/*
class Node {
  public:
    int data;
    Node *next;
    Node *prev;
    Node(int val) {
        data = val;
        next = NULL;
        prev = NULL;
    }
};

*/
class Solution {
  public:
    Node *reverse(Node *head) {
        // code here
        if(head==NULL || head->next==NULL)
            return head;
        Node* temp=head;
        Node* last=NULL;
        while (temp !=NULL)
        {
            last=temp->prev;
            temp->prev=temp->next;
            temp->next=last;
            
            temp=temp->prev;
        }
        return last->prev;
        
    }
};
